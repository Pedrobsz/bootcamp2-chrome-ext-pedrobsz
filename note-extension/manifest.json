{
  "manifest_version": 3,
  "name": "Notepad Extension",
  "version": "0.0.1",
  "description": "Um simples bloco de notas no popup da extens√£o.",
  "icons": {
    "16": "icons/icon.png",
    "32": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "action": {
    "default_title": "Notepad",
    "default_popup": "src/popup/popup.html"
  },
  "background": {
    "service_worker": "src/background/service-worker.js"
  },
  "permissions": ["storage"]
}